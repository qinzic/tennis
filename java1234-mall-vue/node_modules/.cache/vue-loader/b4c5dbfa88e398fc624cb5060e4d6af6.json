" :rules=\"rules\">\r\n\r\n      <el-form-item label=\"用户ID\" prop=\"userId\">\r\n        <el-input v-model=\"form.userId\" style=\"width: 400px\" disabled></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"用户昵称\" prop=\"nickname\">\r\n        <el-input v-model=\"form.nickname\" style=\"width: 400px\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"用户头像\">\r\n        <el-upload\r\n          :headers=\"headers\"\r\n          class=\"avatar-uploader\"\r\n          :action=\"getServerUrl()+'/admin/user/uploadAvatar'\"\r\n          :show-file-list=\"false\"\r\n          :on-success=\"handleAvatarSuccess\"\r\n          :before-upload=\"beforeAvatarUpload\"\r\n        >\r\n          <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\r\n          <el-icon v-else class=\"avatar-uploader-icon\"><plus /></el-icon>\r\n        </el-upload>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"预约类别\" >\r\n        <el-select v-model=\"form.appointmentCategory\" class=\"m-2\" placeholder=\"请选择预约类别...\">\r\n          <el-option\r\n            v-for=\"item in appointmentCategoryOptions\"\r\n            :key=\"item.id\"\r\n            :label=\"item.name\"\r\n            :value=\"item.id\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"用户描述\" prop=\"description\">\r\n        <el-input v-model=\"form.description\" :rows=\"4\" type=\"textarea\" />\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"handleClose\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleConfirm\">确认</el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script setup>\r\nimport { defineEmits, ref, defineProps, watch } from 'vue'\r\nimport axios from \"@/util/axios\"\r\nimport { ElMessage } from \"element-plus\"\r\n\r\nconst props = defineProps({\r\n  dialogTitle: {\r\n    type: String,\r\n    default: '',\r\n    required: true\r\n  },\r\n  dialogValue: {\r\n    type: Object,\r\n    default: () => {}\r\n  }\r\n})\r\n\r\nconst form = ref({\r\n  userId: -1,\r\n  nickname: '',\r\n  appointmentCategory: '',\r\n  avatar: '',\r\n  description: ''\r\n})\r\n\r\nconst imageUrl = ref('')\r\nconst headers = ref({\r\n  token: window.sessionStorage.getItem('token')\r\n})\r\n\r\nconst appointmentCategoryOptions = ref([\r\n  { id: 1, name: '会议预约' },\r\n  { id: 2, name: '体育场地预约' },\r\n  { id: 3, name: '设备预约' }\r\n])\r\n\r\nconst emits = defineEmits(['update:modelValue', 'initUserList'])\r\nconst formRef = ref(null)\r\n\r\nconst rules = ref({\r\n  nickname: [{ required: true, message: '请输入用户昵称！' }],\r\n  appointmentCategory: [{ required: true, message: '请选择预约类别！' }],\r\n  description: [{ required: true, message: '请输入用户描述！' }]\r\n})\r\n\r\nconst handleClose = () => {\r\n  formRef.value.resetFields()\r\n  emits('update:modelValue', false)\r\n}\r\n\r\nconst beforeAvatarUpload = (file) => {\r\n  const isJPG = file.type === 'image/jpeg'\r\n  const isLt2M = file.size / 1024 / 1024 < 2\r\n\r\n  if (!isJPG) {\r\n    ElMessage.error('图片必须是jpg格式')\r\n  }\r\n  if (!isLt2M) {\r\n    ElMessage.error('图片大小不能超过2M!')\r\n  }\r\n  return isJPG && isLt2M\r\n}\r\n\r\nconst handleAvatarSuccess = (res) => {\r\n  imageUrl.value = getServerUrl() + res.data.src\r\n  form.value.avatar = res.data.title\r\n}\r\n\r\nconst handleConfirm = async () => {\r\n  if (form.value.appointmentCategory === '') {\r\n    ElMessage.error('请选择预约类别')\r\n    return\r\n  }\r\n\r\n  formRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      try {\r\n        const result = await axios.post(\"admin/appointment/save\", form.value)\r\n        const data = result.data\r\n        if (data.code === 0) {\r\n          ElMessage.success(\"操作成功\")\r\n          formRef.value.resetFields()\r\n          emits(\"initUserList\")\r\n          handleClose()\r\n        } else {\r\n          ElMessage.error(data.msg)\r\n        }\r\n      } catch (err) {\r\n        console.error(\"error:\", err)\r\n        ElMessage.error('系统运行出错，请联系管理员')\r\n      }\r\n    } else {\r\n      return false\r\n    }\r\n  })\r\n}\r\n\r\nwatch(\r\n  () => props.dialogValue,\r\n  () => {\r\n    form.value = props.dialogValue\r\n  },\r\n  { deep: true, immediate: true }\r\n)\r\n</script>\r\n"]}]}