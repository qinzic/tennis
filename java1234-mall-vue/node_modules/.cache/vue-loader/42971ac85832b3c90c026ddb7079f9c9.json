s/appoint/index.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <el-table :data=\"tableData\" stripe style=\"width: 100%\">\r\n    <el-table-column type=\"index\" width=\"50\" />\r\n\r\n    <el-table-column prop=\"nickname\" label=\"用户昵称\" width=\"200\" />\r\n    <el-table-column prop=\"appointmentCategory\" label=\"预约类别\" width=\"150\" />\r\n    <el-table-column prop=\"description\" label=\"用户描述\" width=\"200\" />\r\n\r\n    <el-table-column prop=\"action\" label=\"操作\" width=\"150\">\r\n      <template v-slot=\"scope\">\r\n        <el-button type=\"danger\" icon=\"el-icon-delete\" @click=\"handleDelete(scope.row.userId)\">\r\n          删除\r\n        </el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-edit\" @click=\"handleEdit(scope.row.userId)\">\r\n          编辑\r\n        </el-button>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n</template>\r\n\r\n<script setup>\r\nimport { defineEmits, ref } from 'vue'\r\nimport { ElMessage } from 'element-plus'\r\nimport axios from '@/util/axios'\r\n\r\nconst tableData = ref([])\r\n\r\nconst emits = defineEmits(['update:modelValue'])\r\n\r\nconst handleDelete = async (userId) => {\r\n  try {\r\n    const result = await axios.delete(`admin/appointment/delete/${userId}`)\r\n    if (result.data.code === 0) {\r\n      ElMessage.success('删除成功')\r\n      fetchTableData()\r\n    } else {\r\n      ElMessage.error(result.data.msg)\r\n    }\r\n  } catch (err) {\r\n    ElMessage.error('删除失败，请稍后再试')\r\n  }\r\n}\r\n\r\nconst handleEdit = (userId) => {\r\n  emits('update:modelValue', userId)\r\n}\r\n\r\nconst fetchTableData = async () => {\r\n  try {\r\n    const result = await axios.get('admin/appointment/list')\r\n    tableData.value = result.data.data\r\n  } catch (err) {\r\n    ElMessage.error('数据加载失败')\r\n  }\r\n}\r\n\r\nfetchTableData()\r\n</script>\r\n"]}]}